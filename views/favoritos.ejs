<!DOCTYPE html>
<html lang="pt-br">
<!--HEAD DA PAGINA-->
<%- include('./partials/head') %>

	<body>
		<!-- HEADER -->
		<%- include('./partials/header') %>
			<!-- NAVIGATION -->
			<%- include('./partials/nav') %>

				<!-- SECTION -->
				<div class="section">
					<!-- container -->
					<div class="container">
						<!-- row -->
						<div class="row">

							<!-- section title -->
							<div class="col-md-12">
								<div class="section-title">
									<h3 class="title">SEUS ANÚNCIOS FAVORITOS</h3>

								</div>
							</div>
							<!-- /section title -->
							<!-- Products tab & slick -->
							<div class="col-md-12">
								<div class="row">

									<div class="products-tabs">
										<!-- tab -->
										<div id="tab2" class="tab-pane fade in active">
											<div class="products-slick" data-nav="#slick-nav-2">
												<!-- product -->
												<% favoritos.forEach(favorito=>{ %>

											
												
													<div class="product">
														<div class="product-img">
															<img src="<%= favorito.imagens[0].imagem %>" alt="">
															console.log(<%= favorito.imagens[0].imagem %>)

														</div>
														<div class="product-body">
															<p class="product-category">Eletrônicos</p>
															<h3 class="product-name"><a href="#">
																	<%= favorito.anuncios_favoritos.titulo %>
																</a></h3>
															<h4 class="product-price">
																<%= favorito.anuncios_favoritos.valor %>
															</h4>
														</div>
														<div class="add-to-cart">
															<button class="add-to-cart-btn"><i
																	class="fa fa-star"></i>Desfavoritar</button>
														</div>
													</div>
													<%})%>
														<!-- /product -->



											</div>
											<div id="slick-nav-2" class="products-slick-nav"></div>
										</div>
										<!-- /tab -->
									</div>
								</div>
							</div>
							<!-- /Products tab & slick -->
						</div>
						<!-- /row -->
					</div>
					<!-- /container -->
				</div>
				<!-- /SECTION -->


				<%- include('./partials/redesSociais') %>
					<%- include('./partials/footer') %>
	</body>

</html>