<!DOCTYPE html>
<html lang="pt-br">
<!--HEAD DA PAGINA-->
<%- include('./partials/head') %>

<body>
	<!-- HEADER -->
	<%- include('./partials/header') %>
	<!-- NAVIGATION -->
	<%- include('./partials/nav') %>

		<!-- BREADCRUMB -->
		<div id="breadcrumb" class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">
					<div class="col-md-12">
						<h3 class="breadcrumb-header">O que você quer vender?</h3>
						
					</div>
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /BREADCRUMB -->

		<!-- SECTION -->
		<div class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">

					<div class="col-md-7" >
						<!-- Billing Details -->
						<div class="billing-details" >
							<div class="section-title">
								<h3 class="title">Anunciar</h3>
							</div>
							<div class="form-group">
								<input class="input" type="title" name="title" placeholder="Título do produto">
							</div>
							<div class="form-group">
								<textarea class = "input" name="description" placeholder="Descrição do produto"></textarea>
							</div>
							<div class="form-group">
								<form>
									<select class="input-select">
										<option value="0">Categorias</option>
										<option value="1">Cadeiras</option>
										<option value="1">Mesas</option>
										<option value="1">Armários</option>
										<option value="1">Camas</option>
										<option value="1">Acessórios</option>
										<option value="1">Eletrônicos</option>
									</select>
								</form>
							</div>

							<div class="form-group">
								<label for="file" class="input" id="file-input">Escolha uma imagem</label>
								<input class="input" type="file" name="file" id="file" placeholder="Imagem do produto">
							</div>

							<div class="form-group">
								<form>
									<select class="input-select">
										<option value="0">Localização</option>
										<option value="1">Endereço 1</option>
										<option value="1">Endereço 2</option>
									</select>
								</form>
							</div>

							<div class="form-group">
								<input class="input" type="tel" name="tel" placeholder="Seu telefone" >
							</div>
							
							
							<a href="#" class="primary-btn order-submit">Vender</a>
						</div>

						
						<!-- /Billing Details -->

						
					
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /SECTION -->



		<%- include('./partials/redesSociais') %>
		<%- include('./partials/footer') %>
	</body>
</html>