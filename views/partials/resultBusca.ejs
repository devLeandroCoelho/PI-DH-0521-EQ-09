		<!-- SECTION -->
		<div class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">

				
					<div class="col-md-12">
						<div class="section-title">
							<h3 class="title">Resultado de Busca para <%=nomeBusca;%> </h3>

						</div>
					</div>
					<!-- /section title -->

					<!-- Products tab & slick -->
					<div class="col-md-12">
						<div class="row">
							

							<div class="products-tabs">
								<!-- tab -->
								<div id="tab2" class="tab-pane fade in active">
									<div class="products-slick" data-nav="#slick-nav-2">
										<!-- product -->
										<% resultBusca.forEach(resultBusca=>{ %>
										<div class="product">
											<a href="/produto/<%= resultBusca.id %>">
											<div class="product-img">
												<img src="<%= resultBusca.imagem %>" alt="">

											</div>
											<div class="product-body">
												<p class="product-category"><%= resultBusca.categoria_nome %></p>
												<h3 class="product-name"><a href="/produto/<%= resultBusca.id %>"><%= resultBusca.titulo %> </a></h3>
												<h4 class="product-price"><%= resultBusca.valor.toLocaleString('pt-br',{style: 'currency', currency: 'BRL'})%></h4>
											</div>
											<div class="add-to-cart">
												<button class="add-to-cart-btn"><i class="fa fa-star"><a href="/produto/<%= resultBusca.id %>"></i>Conferir</button></a>
											</div>
										</a>
										</div>
									
										<%})%>
										<!-- /product -->

									</div>
									<div id="slick-nav-2" class="products-slick-nav"></div>
								</div>
								<!-- /tab -->
							</div>
						</div>
					</div>
					<!-- /Products tab & slick -->
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->

		
		</div>
		<!-- /SECTION -->